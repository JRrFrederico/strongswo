[
  {
    "livro": "Gênesis - בְּרֵאשִׁית - Bereshit",
    "capitulo": 1,
    "titulos": {},
    "versiculos": {
      "1": {
        "traducao_completa": "No princípio, criou Deus os céus e a terra.",
        "palavras": [
          { "hebraico": "בְּרֵאשִׁית", "transliteral": "BERESHIT", "traducao_palavra": "No princípio" },
          { "hebraico": "בָּרָא", "transliteral": "BARA", "traducao_palavra": "criou" },
          { "hebraico": "אֱלֹהִים", "transliteral": "ELOHIM", "traducao_palavra": "Deus" },
          { "hebraico": "אֵת הַשָּׁמַיִם", "transliteral": "ET HASHAMAYIM", "traducao_palavra": "os céus" },
          { "hebraico": "וְאֵת הָאָרֶץ", "transliteral": "VE'ET HA'ARETZ", "traducao_palavra": "e a terra" }
        ]
      },
      "2": {
        "traducao_completa": "E a terra era sem forma e vazia; e havia trevas sobre a face do abismo; e o Espírito de Deus se movia sobre a face das águas.",
        "palavras": [
          { "hebraico": "וְהָאָרֶץ", "transliteral": "VEHA'ARETZ", "traducao_palavra": "E a terra" },
          { "hebraico": "הָיְתָה", "transliteral": "HAYTAH", "traducao_palavra": "era" },
          { "hebraico": "תֹהוּ וָבֹהוּ", "transliteral": "TOHU VAVOHU", "traducao_palavra": "sem forma e vazia" },
          { "hebraico": "וְחֹשֶׁךְ", "transliteral": "VECHOSHECH", "traducao_palavra": "e havia trevas" },
          { "hebraico": "עַל פְּנֵי תְהוֹם", "transliteral": "AL PENEI TEHOM", "traducao_palavra": "sobre a face do abismo" },
          { "hebraico": "וְרוּחַ אֱלֹהִים", "transliteral": "VERUACH ELOHIM", "traducao_palavra": "e o Espírito de Deus" },
          { "hebraico": "מְרַחֶפֶת", "transliteral": "MERACHEFET", "traducao_palavra": "se movia" },
          { "hebraico": "עַל פְּנֵי הַמָּיִם", "transliteral": "AL PENEI HAMAYIM", "traducao_palavra": "sobre a face das águas" }
        ]
      },
      "3": {
        "traducao_completa": "E disse Deus: Haja luz; e houve luz.",
        "palavras": [
          { "hebraico": "וַיֹּאמֶר אֱלֹהִים", "transliteral": "VAYOMER ELOHIM", "traducao_palavra": "E disse Deus" },
          { "hebraico": "יְהִי אוֹר", "transliteral": "YEHI OR", "traducao_palavra": "Haja luz" },
          { "hebraico": "וַיְהִי אוֹר", "transliteral": "VAYHI OR", "traducao_palavra": "e houve luz" }
        ]
      },
      "4": {
        "traducao_completa": "E viu Deus que a luz era boa; e Deus separou a luz das trevas.",
        "palavras": [
          { "hebraico": "וַיַּרְא אֱלֹהִים", "transliteral": "VAYAR ELOHIM", "traducao_palavra": "E viu Deus" },
          { "hebraico": "אֶת הָאוֹר", "transliteral": "ET HA'OR", "traducao_palavra": "que a luz" },
          { "hebraico": "כִּי טוֹב", "transliteral": "KI TOV", "traducao_palavra": "era boa" },
          { "hebraico": "וַיַּבְדֵּל אֱלֹהִים", "transliteral": "VAYAVDEL ELOHIM", "traducao_palavra": "e Deus separou" },
          { "hebraico": "בֵּין הָאוֹר", "transliteral": "BEIN HA'OR", "traducao_palavra": "a luz" },
          { "hebraico": "וּבֵין הַחֹשֶׁךְ", "transliteral": "UBEIN HACHOSHECH", "traducao_palavra": "das trevas" }
        ]
      },
      "5": {
        "traducao_completa": "E Deus chamou à luz Dia, e às trevas chamou Noite. E foi a tarde e a manhã, o dia primeiro.",
        "palavras": [
          { "hebraico": "וַיִּקְרָא אֱלֹהִים", "transliteral": "VAYIQRA ELOHIM", "traducao_palavra": "E Deus chamou" },
          { "hebraico": "לָאוֹר יוֹם", "transliteral": "LA'OR YOM", "traducao_palavra": "à luz Dia" },
          { "hebraico": "וְלַחֹשֶׁךְ קָרָא לָיְלָה", "transliteral": "VELACHOSHECH QARA LAYLAH", "traducao_palavra": "e às trevas chamou Noite" },
          { "hebraico": "וַיְהִי עֶרֶב וַיְהִי בֹקֶר", "transliteral": "VAYHI EREV VAYHI BOQER", "traducao_palavra": "E foi a tarde e a manhã" },
          { "hebraico": "יוֹם אֶחָד", "transliteral": "YOM ECHAD", "traducao_palavra": "o dia primeiro" }
        ]
      },
      "6": {
        "traducao_completa": "E disse Deus: Haja uma expansão no meio das águas, e haja separação entre águas e águas.",
        "palavras": [
          { "hebraico": "וַיֹּאמֶר אֱלֹהִים", "transliteral": "VAYOMER ELOHIM", "traducao_palavra": "E disse Deus" },
          { "hebraico": "יְהִי רָקִיעַ", "transliteral": "YEHI RAQIA", "traducao_palavra": "Haja uma expansão" },
          { "hebraico": "בְּתוֹךְ הַמָּיִם", "transliteral": "BETOCH HAMAYIM", "traducao_palavra": "no meio das águas" },
          { "hebraico": "וִיהִי מַבְדִּיל", "transliteral": "VIHI MAVDIL", "traducao_palavra": "e haja separação" },
          { "hebraico": "בֵּין מַיִם לָמָיִם", "transliteral": "BEIN MAYIM LAMAYIM", "traducao_palavra": "entre águas e águas" }
        ]
      },
      "7": {
        "traducao_completa": "E fez Deus a expansão, e separou as águas que estavam debaixo da expansão das águas que estavam sobre a expansão; e assim foi.",
        "palavras": [
          { "hebraico": "וַיַּעַשׂ אֱלֹהִים", "transliteral": "VAYA'AS ELOHIM", "traducao_palavra": "E fez Deus" },
          { "hebraico": "אֶת הָרָקִיעַ", "transliteral": "ET HARAQIA", "traducao_palavra": "a expansão" },
          { "hebraico": "וַיַּבְדֵּל", "transliteral": "VAYAVDEL", "traducao_palavra": "e separou" },
          { "hebraico": "בֵּין הַמַּיִם", "transliteral": "BEIN HAMAYIM", "traducao_palavra": "as águas" },
          { "hebraico": "אֲשֶׁר מִתַּחַת לָרָקִיעַ", "transliteral": "ASHER MITACHAT LARAQIA", "traducao_palavra": "que estavam debaixo da expansão" },
          { "hebraico": "וּבֵין הַמַּיִם", "transliteral": "UBEIN HAMAYIM", "traducao_palavra": "das águas" },
          { "hebraico": "אֲשֶׁר מֵעַל לָרָקִיעַ", "transliteral": "ASHER ME'AL LARAQIA", "traducao_palavra": "que estavam sobre a expansão" },
          { "hebraico": "וַיְהִי כֵן", "transliteral": "VAYHI KEN", "traducao_palavra": "e assim foi" }
        ]
      },
      "8": {
        "traducao_completa": "E Deus chamou à expansão Céus; e foi a tarde e a manhã, o dia segundo.",
        "palavras": [
          { "hebraico": "וַיִּקְרָא אֱלֹהִים", "transliteral": "VAYIQRA ELOHIM", "traducao_palavra": "E Deus chamou" },
          { "hebraico": "לָרָקִיעַ שָׁמָיִם", "transliteral": "LARAQIA SHAMAYIM", "traducao_palavra": "à expansão Céus" },
          { "hebraico": "וַיְהִי עֶרֶב וַיְהִי בֹקֶר", "transliteral": "VAYHI EREV VAYHI BOQER", "traducao_palavra": "e foi a tarde e a manhã" },
          { "hebraico": "יוֹם שֵׁנִי", "transliteral": "YOM SHENI", "traducao_palavra": "o dia segundo" }
        ]
      },
      "9": {
        "traducao_completa": "E disse Deus: Ajuntem-se as águas debaixo dos céus num lugar; e apareça a porção seca; e assim foi.",
        "palavras": [
          { "hebraico": "וַיֹּאמֶר אֱלֹהִים", "transliteral": "VAYOMER ELOHIM", "traducao_palavra": "E disse Deus" },
          { "hebraico": "יִקָּווּ הַמַּיִם", "transliteral": "YIQQAVU HAMAYIM", "traducao_palavra": "Ajuntem-se as águas" },
          { "hebraico": "מִתַּחַת הַשָּׁמַיִם", "transliteral": "MITACHAT HASHAMAYIM", "traducao_palavra": "debaixo dos céus" },
          { "hebraico": "אֶל מָקוֹם אֶחָד", "transliteral": "EL MAQOM ECHAD", "traducao_palavra": "num lugar" },
          { "hebraico": "וְתֵרָאֶה הַיַּבָּשָׁה", "transliteral": "VETERAHEH HAYABBASHAH", "traducao_palavra": "e apareça a porção seca" },
          { "hebraico": "וַיְהִי כֵן", "transliteral": "VAYHI KEN", "traducao_palavra": "e assim foi" }
        ]
      },
      "10": {
        "traducao_completa": "E Deus chamou à porção seca Terra; e ao ajuntamento das águas chamou Mares; e viu Deus que era bom.",
        "palavras": [
          { "hebraico": "וַיִּקְרָא אֱלֹהִים", "transliteral": "VAYIQRA ELOHIM", "traducao_palavra": "E Deus chamou" },
          { "hebraico": "לַיַּבָּשָׁה אֶרֶץ", "transliteral": "LAYABBASHAH ERETZ", "traducao_palavra": "à porção seca Terra" },
          { "hebraico": "וּלְמִקְוֵה הַמַּיִם", "transliteral": "ULEMIQVEH HAMAYIM", "traducao_palavra": "e ao ajuntamento das águas" },
          { "hebraico": "קָרָא יַמִּים", "transliteral": "QARA YAMIM", "traducao_palavra": "chamou Mares" },
          { "hebraico": "וַיַּרְא אֱלֹהִים", "transliteral": "VAYAR ELOHIM", "traducao_palavra": "e viu Deus" },
          { "hebraico": "כִּי טוֹב", "transliteral": "KI TOV", "traducao_palavra": "que era bom" }
        ]
      },
      "11": {
        "traducao_completa": "E disse Deus: Produza a terra erva verde, erva que dê semente, árvore frutífera que dê fruto segundo a sua espécie, cuja semente esteja nela sobre a terra; e assim foi.",
        "palavras": [
          { "hebraico": "וַיֹּאמֶר אֱלֹהִים", "transliteral": "VAYOMER ELOHIM", "traducao_palavra": "E disse Deus" },
          { "hebraico": "תַּדְשֵׁא הָאָרֶץ", "transliteral": "TADSHÉ HA'ARETZ", "traducao_palavra": "Produza a terra" },
          { "hebraico": "דֶּשֶׁא עֵשֶׂב", "transliteral": "DESHE ESEV", "traducao_palavra": "erva verde, erva" },
          { "hebraico": "מַזְרִיעַ זֶרַע", "transliteral": "MAZRIA ZERA", "traducao_palavra": "que dê semente" },
          { "hebraico": "עֵץ פְּרִי עֹשֶׂה", "transliteral": "ETZ PERI OSEH", "traducao_palavra": "árvore frutífera que dê fruto" },
          { "hebraico": "פְּרִי לְמִינוֹ", "transliteral": "PERI LEMINO", "traducao_palavra": "segundo a sua espécie" },
          { "hebraico": "אֲשֶׁר זַרְעוֹ בוֹ", "transliteral": "ASHER ZAR'O BO", "traducao_palavra": "cuja semente esteja nela" },
          { "hebraico": "עַל הָאָרֶץ", "transliteral": "AL HA'ARETZ", "traducao_palavra": "sobre a terra" },
          { "hebraico": "וַיְהִי כֵן", "transliteral": "VAYHI KEN", "traducao_palavra": "e assim foi" }
        ]
      },
      "12": {
        "traducao_completa": "E a terra produziu erva, erva que dá semente segundo a sua espécie, e árvore frutífera cuja semente está nela, segundo a sua espécie; e viu Deus que era bom.",
        "palavras": [
          { "hebraico": "וַתּוֹצֵא הָאָרֶץ", "transliteral": "VATOTZE HA'ARETZ", "traducao_palavra": "E a terra produziu" },
          { "hebraico": "דֶּשֶׁא עֵשֶׂב", "transliteral": "DESHE ESEV", "traducao_palavra": "erva, erva" },
          { "hebraico": "מַזְרִיעַ זָרַע", "transliteral": "MAZRIA ZARA", "traducao_palavra": "que dá semente" },
          { "hebraico": "לְמִינֵהוּ", "transliteral": "LEMINEHU", "traducao_palavra": "segundo a sua espécie" },
          { "hebraico": "וְעֵץ עֹשֶׂה", "transliteral": "VE'ETZ OSEH", "traducao_palavra": "e árvore frutífera" },
          { "hebraico": "פְּרִי אֲשֶׁר זַרְעוֹ בוֹ", "transliteral": "PERI ASHER ZAR'O BO", "traducao_palavra": "cuja semente está nela" },
          { "hebraico": "לְמִינֵהוּ", "transliteral": "LEMINEHU", "traducao_palavra": "segundo a sua espécie" },
          { "hebraico": "וַיַּרְא אֱלֹהִים", "transliteral": "VAYAR ELOHIM", "traducao_palavra": "e viu Deus" },
          { "hebraico": "כִּי טוֹב", "transliteral": "KI TOV", "traducao_palavra": "que era bom" }
        ]
      },
      "13": {
        "traducao_completa": "E foi a tarde e a manhã, o dia terceiro.",
        "palavras": [
          { "hebraico": "וַיְהִי עֶרֶב וַיְהִי בֹקֶר", "transliteral": "VAYHI EREV VAYHI BOQER", "traducao_palavra": "E foi a tarde e a manhã" },
          { "hebraico": "יוֹם שְׁלִישִׁי", "transliteral": "YOM SHELISHI", "traducao_palavra": "o dia terceiro" }
        ]
      },
      "14": {
        "traducao_completa": "E disse Deus: Haja luminares na expansão dos céus, para fazerem separação entre o dia e a noite; e sejam eles para sinais e para tempos determinados, e para dias e anos;",
        "palavras": [
          { "hebraico": "וַיֹּאמֶר אֱלֹהִים", "transliteral": "VAYOMER ELOHIM", "traducao_palavra": "E disse Deus" },
          { "hebraico": "יְהִי מְאֹרֹת", "transliteral": "YEHI ME'OROT", "traducao_palavra": "Haja luminares" },
          { "hebraico": "בְּרָקִיעַ הַשָּׁמָיִם", "transliteral": "BERAQIA HASHAMAYIM", "traducao_palavra": "na expansão dos céus" },
          { "hebraico": "לְהַבְדִּיל", "transliteral": "LEHAVDIL", "traducao_palavra": "para fazerem separação" },
          { "hebraico": "בֵּין הַיּוֹם", "transliteral": "BEIN HAYYOM", "traducao_palavra": "entre o dia" },
          { "hebraico": "וּבֵין הַלַּיְלָה", "transliteral": "UBEIN HALLAYLAH", "traducao_palavra": "e a noite" },
          { "hebraico": "וְהָיוּ לְאֹתֹת", "transliteral": "VEHAYU LE'OTOT", "traducao_palavra": "e sejam eles para sinais" },
          { "hebraico": "וּלְמוֹעֲדִים", "transliteral": "ULEMO'ADIM", "traducao_palavra": "e para tempos determinados" },
          { "hebraico": "וּלְיָמִים וְשָׁנִים", "transliteral": "ULEYAMIM VESHANIM", "traducao_palavra": "e para dias e anos" }
        ]
      },
      "15": {
        "traducao_completa": "E sejam por luminares na expansão dos céus, para alumiar a terra; e assim foi.",
        "palavras": [
          { "hebraico": "וְהָיוּ לִמְאוֹרֹת", "transliteral": "VEHAYU LIM'OROT", "traducao_palavra": "E sejam por luminares" },
          { "hebraico": "בְּרָקִיעַ הַשָּׁמָיִם", "transliteral": "BERAQIA HASHAMAYIM", "traducao_palavra": "na expansão dos céus" },
          { "hebraico": "לְהָאִיר עַל הָאָרֶץ", "transliteral": "LEHA'IR AL HA'ARETZ", "traducao_palavra": "para alumiar a terra" },
          { "hebraico": "וַיְהִי כֵן", "transliteral": "VAYHI KEN", "traducao_palavra": "e assim foi" }
        ]
      },
      "16": {
        "traducao_completa": "E fez Deus os dois grandes luminares: o maior luminar para governar o dia, e o menor luminar para governar a noite; fez também as estrelas.",
        "palavras": [
          { "hebraico": "וַיַּעַשׂ אֱלֹהִים", "transliteral": "VAYA'AS ELOHIM", "traducao_palavra": "E fez Deus" },
          { "hebraico": "אֶת שְׁנֵי הַמְּאֹרֹת", "transliteral": "ET SHNEI HAME'OROT", "traducao_palavra": "os dois grandes luminares" },
          { "hebraico": "הַגְּדֹלִים", "transliteral": "HAGGEDOLIM", "traducao_palavra": "" },
          { "hebraico": "אֶת הַמָּאוֹר הַגָּדֹל", "transliteral": "ET HAMA'OR HAGGADOL", "traducao_palavra": "o maior luminar" },
          { "hebraico": "לְמֶמְשֶׁלֶת הַיּוֹם", "transliteral": "LEMEMSHELET HAYYOM", "traducao_palavra": "para governar o dia" },
          { "hebraico": "וְאֶת הַמָּאוֹר הַקָּטֹן", "transliteral": "VE'ET HAMA'OR HAQATON", "traducao_palavra": "e o menor luminar" },
          { "hebraico": "לְמֶמְשֶׁלֶת הַלַּיְלָה", "transliteral": "LEMEMSHELET HALLAYLAH", "traducao_palavra": "para governar a noite" },
          { "hebraico": "וְאֵת הַכּוֹכָבִים", "transliteral": "VE'ET HAKOCHAVIM", "traducao_palavra": "fez também as estrelas" }
        ]
      },
      "17": {
        "traducao_completa": "E Deus os pôs na expansão dos céus para alumiar a terra,",
        "palavras": [
          { "hebraico": "וַיִּתֵּן אֹתָם אֱלֹהִים", "transliteral": "VAYITEN OTAM ELOHIM", "traducao_palavra": "E Deus os pôs" },
          { "hebraico": "בְּרָקִיעַ הַשָּׁמָיִם", "transliteral": "BERAQIA HASHAMAYIM", "traducao_palavra": "na expansão dos céus" },
          { "hebraico": "לְהָאִיר עַל הָאָרֶץ", "transliteral": "LEHA'IR AL HA'ARETZ", "traducao_palavra": "para alumiar a terra" }
        ]
      },
      "18": {
        "traducao_completa": "E para governar o dia e a noite, e para fazer separação entre a luz e as trevas; e viu Deus que era bom.",
        "palavras": [
          { "hebraico": "וְלִמְשֹׁל בַּיּוֹם וּבַלַּיְלָה", "transliteral": "VELIMSHOL BAYYOM UVALAYLAH", "traducao_palavra": "E para governar o dia e a noite" },
          { "hebraico": "וּלְהַבְדִּיל בֵּין הָאוֹר", "transliteral": "ULEHAVDIL BEIN HA'OR", "traducao_palavra": "e para fazer separação entre a luz" },
          { "hebraico": "וּבֵין הַחֹשֶׁךְ", "transliteral": "UBEIN HACHOSHECH", "traducao_palavra": "e as trevas" },
          { "hebraico": "וַיַּרְא אֱלֹהִים", "transliteral": "VAYAR ELOHIM", "traducao_palavra": "e viu Deus" },
          { "hebraico": "כִּי טוֹב", "transliteral": "KI TOV", "traducao_palavra": "que era bom" }
        ]
      },
      "19": {
