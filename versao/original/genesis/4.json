{
  "livro": "Gênesis",
  "capitulo": 4,
  "versiculos": {
    "1": {
      "traducao_completa": "E conheceu Adão a Eva, sua mulher, e ela concebeu, e deu à luz a Caim, e disse: Alcancei do SENHOR um varão.",
      "palavras": [
        { "hebraico": "וְהָאָדָם יָדַע", "transliteral": "VEHA'ADAM YADA", "traducao_palavra": "E conheceu Adão" },
        { "hebraico": "אֶת חַוָּה אִשְׁתּוֹ", "transliteral": "ET CHAVVAH ISHTO", "traducao_palavra": "a Eva, sua mulher" },
        { "hebraico": "וַתַּהַר", "transliteral": "VATTAHAR", "traducao_palavra": "e ela concebeu" },
        { "hebraico": "וַתֵּלֶד", "transliteral": "VATTELED", "traducao_palavra": "e deu à luz" },
        { "hebraico": "אֶת קַיִן", "transliteral": "ET QAYIN", "traducao_palavra": "a Caim" },
        { "hebraico": "וַתֹּאמֶר", "transliteral": "VATOMER", "traducao_palavra": "e disse" },
        { "hebraico": "קָנִיתִי אִישׁ", "transliteral": "QANITI ISH", "traducao_palavra": "Alcancei um varão" },
        { "hebraico": "אֶת יְהוָה", "transliteral": "ET YHVH", "traducao_palavra": "do SENHOR" }
      ]
    },
    "2": {
      "traducao_completa": "E deu à luz mais a seu irmão Abel; e Abel foi pastor de ovelhas, e Caim foi lavrador da terra.",
      "palavras": [
        { "hebraico": "וַתֹּסֶף לָלֶדֶת", "transliteral": "VATTOSEF LALEDET", "traducao_palavra": "E deu à luz mais" },
        { "hebraico": "אֶת אָחִיו אֶת הֶבֶל", "transliteral": "ET ACHIYV ET HEVEL", "traducao_palavra": "a seu irmão Abel" },
        { "hebraico": "וַיְהִי הֶבֶל", "transliteral": "VAYHI HEVEL", "traducao_palavra": "e Abel foi" },
        { "hebraico": "רֹעֵה צֹאן", "transliteral": "RO'EH TZON", "traducao_palavra": "pastor de ovelhas" },
        { "hebraico": "וְקַיִן הָיָה עֹבֵד אֲדָמָה", "transliteral": "VEQAYIN HAYA OVED ADAMAH", "traducao_palavra": "e Caim foi lavrador da terra" }
      ]
    },
    "3": {
      "traducao_completa": "E aconteceu, ao cabo de dias, que Caim trouxe do fruto da terra uma oferta ao SENHOR.",
      "palavras": [
        { "hebraico": "וַיְהִי מִקֵּץ יָמִים", "transliteral": "VAYHI MIQQETZ YAMIM", "traducao_palavra": "E aconteceu, ao cabo de dias" },
        { "hebraico": "וַיָּבֵא קַיִן", "transliteral": "VAYYAVE QAYIN", "traducao_palavra": "que Caim trouxe" },
        { "hebraico": "מִפְּרִי הָאֲדָמָה", "transliteral": "MIPRI HA'ADAMAH", "traducao_palavra": "do fruto da terra" },
        { "hebraico": "מִנְחָה לַיהוָה", "transliteral": "MINCHAH LA-YHVH", "traducao_palavra": "uma oferta ao SENHOR" }
      ]
    },
    "4": {
      "traducao_completa": "E Abel também trouxe dos primogênitos das suas ovelhas, e da sua gordura; e atentou o SENHOR para Abel e para a sua oferta;",
      "palavras": [
        { "hebraico": "וְהֶבֶל הֵבִיא גַם הוּא", "transliteral": "VEHEVEL HEVI GAM HU", "traducao_palavra": "E Abel também trouxe" },
        { "hebraico": "מִבְּכֹרוֹת צֹאנוֹ", "transliteral": "MIBCHOROT TZONO", "traducao_palavra": "dos primogênitos das suas ovelhas" },
        { "hebraico": "וּמֵחֶלְבֵהֶן", "transliteral": "UMECHELBEHEN", "traducao_palavra": "e da sua gordura" },
        { "hebraico": "וַיִּשַׁע יְהוָה אֶל הֶבֶל", "transliteral": "VAYYISHA YHVH EL HEVEL", "traducao_palavra": "e atentou o SENHOR para Abel" },
        { "hebraico": "וְאֶל מִנְחָתוֹ", "transliteral": "VE'EL MINCHATO", "traducao_palavra": "e para a sua oferta" }
      ]
    },
    "5": {
      "traducao_completa": "Mas para Caim e para a sua oferta não atentou. E irou-se Caim muito, e descaiu-lhe o semblante.",
      "palavras": [
        { "hebraico": "וְאֶל קַיִן וְאֶל מִנְחָתוֹ", "transliteral": "VE'EL QAYIN VE'EL MINCHATO", "traducao_palavra": "Mas para Caim e para a sua oferta" },
        { "hebraico": "לֹא שָׁעָה", "transliteral": "LO SHA'AH", "traducao_palavra": "não atentou" },
        { "hebraico": "וַיִּחַר לְקַיִן מְאֹד", "transliteral": "VAYYICHAR LEQAYIN ME'OD", "traducao_palavra": "E irou-se Caim muito" },
        { "hebraico": "וַיִּפְּלוּ פָּנָיו", "transliteral": "VAYYIPPU PANAV", "traducao_palavra": "e descaiu-lhe o semblante" }
      ]
    },
    "6": {
      "traducao_completa": "E o SENHOR disse a Caim: Por que te iras? e por que descaiu o teu semblante?",
      "palavras": [
        { "hebraico": "וַיֹּאמֶר יְהוָה אֶל קָיִן", "transliteral": "VAYOMER YHVH EL QAYIN", "traducao_palavra": "E o SENHOR disse a Caim" },
        { "hebraico": "לָמָּה חָרָה לָךְ", "transliteral": "LAMAH CHARAH LACH", "traducao_palavra": "Por que te iras" },
        { "hebraico": "וְלָמָּה נָפְלוּ פָנֶיךָ", "transliteral": "VELAMAH NAFELU PANAYCHA", "traducao_palavra": "e por que descaiu o teu semblante" }
      ]
    },
    "7": {
      "traducao_completa": "Se bem fizeres, não haverá aceitação? e se não bem fizeres, ao pecado jaz à porta, e sobre ti será o seu desejo, mas sobre ele tu deverás dominar.",
      "palavras": [
        { "hebraico": "הֲלוֹא אִם תֵּיטִיב שְׂאֵת", "transliteral": "HALO IM TEYTIV SE'ET", "traducao_palavra": "Se bem fizeres, não haverá aceitação" },
        { "hebraico": "וְאִם לֹא תֵיטִיב", "transliteral": "VE'IM LO TEYTIV", "traducao_palavra": "e se não bem fizeres" },
        { "hebraico": "לַפֶּתַח חַטָּאת רֹבֵץ", "transliteral": "LAPETACH CHATTAT ROVEZTZ", "traducao_palavra": "ao pecado jaz à porta" },
        { "hebraico": "וְאֵלֶיךָ תְּשׁוּקָתוֹ", "transliteral": "VE'ELECHA TESHUQATO", "traducao_palavra": "e sobre ti será o seu desejo" },
        { "hebraico": "וְאַתָּה תִּמְשָׁל בּוֹ", "transliteral": "VE'ATTAH TIMSHOL BO", "traducao_palavra": "mas sobre ele tu deverás dominar" }
      ]
    },
    "8": {
      "traducao_completa": "E falou Caim com Abel, seu irmão; e aconteceu que, estando eles no campo, se levantou Caim contra Abel, seu irmão, e o matou.",
      "palavras": [
        { "hebraico": "וַיֹּאמֶר קַיִן אֶל הֶבֶל אָחִיו", "transliteral": "VAYOMER QAYIN EL HEVEL ACHIV", "traducao_palavra": "E falou Caim com Abel, seu irmão" },
        { "hebraico": "וַיְהִי בִּהְיוֹתָם בַּשָּׂדֶה", "transliteral": "VAYHI BIHEYOTAM BASADEH", "traducao_palavra": "e aconteceu que, estando eles no campo" },
        { "hebraico": "וַיָּקָם קַיִן אֶל הֶבֶל אָחִיו", "transliteral": "VAYYAQOM QAYIN EL HEVEL ACHIV", "traducao_palavra": "se levantou Caim contra Abel, seu irmão" },
        { "hebraico": "וַיַּהַרְגֵהוּ", "transliteral": "VAYYAHARGEHU", "traducao_palavra": "e o matou" }
      ]
    },
    "9": {
      "traducao_completa": "E disse o SENHOR a Caim: Onde está Abel, teu irmão? E ele disse: Não sei; sou eu guardador do meu irmão?",
      "palavras": [
        { "hebraico": "וַיֹּאמֶר יְהוָה אֶל קָיִן", "transliteral": "VAYOMER YHVH EL QAYIN", "traducao_palavra": "E disse o SENHOR a Caim" },
        { "hebraico": "אַיֵּה הֶבֶל אָחִיךָ", "transliteral": "AYYEH HEVEL ACHICHA", "traducao_palavra": "Onde está Abel, teu irmão" },
        { "hebraico": "וַיֹּאמֶר", "transliteral": "VAYOMER", "traducao_palavra": "E ele disse" },
        { "hebraico": "לֹא יָדַעְתִּי", "transliteral": "LO YADA'TI", "traducao_palavra": "Não sei" },
        { "hebraico": "הֲשֹׁמֵר אָחִי אָנֹכִי", "transliteral": "HASHOMER ACHY ANOCHI", "traducao_palavra": "sou eu guardador do meu irmão" }
      ]
    },
    "10": {
      "traducao_completa": "E disse: Que fizeste? A voz do sangue do teu irmão clama a mim desde a terra.",
      "palavras": [
        { "hebraico": "וַיֹּאמֶר", "transliteral": "VAYOMER", "traducao_palavra": "E disse" },
        { "hebraico": "מַה עָשִׂיתָ", "transliteral": "MAH ASITA", "traducao_palavra": "Que fizeste" },
        { "hebraico": "קוֹל דְּמֵי אָחִיךָ", "transliteral": "QOL DMEI ACHICHA", "traducao_palavra": "A voz do sangue do teu irmão" },
        { "hebraico": "צֹעֲקִים אֵלַי", "transliteral": "TZO'AQIM ELAY", "traducao_palavra": "clama a mim" },
        { "hebraico": "מִן הָאֲדָמָה", "transliteral": "MIN HA'ADAMAH", "traducao_palavra": "desde a terra" }
      ]
    },
    "11": {
      "traducao_completa": "E agora maldito és tu desde a terra, que abriu a sua boca para receber o sangue de teu irmão de tuas mãos;",
      "palavras": [
        { "hebraico": "וְעַתָּה אָרוּר אָתָּה", "transliteral": "VE'ATTAH ARUR ATTAH", "traducao_palavra": "E agora maldito és tu" },
        { "hebraico": "מִן הָאֲדָמָה", "transliteral": "MIN HA'ADAMAH", "traducao_palavra": "desde a terra" },
        { "hebraico": "אֲשֶׁר פָּצְתָה אֶת פִּיהָ", "transliteral": "ASHER PATZTAH ET PIHA", "traducao_palavra": "que abriu a sua boca" },
        { "hebraico": "לָקַחַת אֶת דַּם אָחִיךָ", "transliteral": "LAQACHAT ET DAM ACHICHA", "traducao_palavra": "para receber o sangue de teu irmão" },
        { "hebraico": "מִיָּדֶךָ", "transliteral": "MIYYADECHA", "traducao_palavra": "de tuas mãos" }
      ]
    },
    "12": {
      "traducao_completa": "Quando lavrares a terra, não te dará mais a sua força; fugitivo e errante serás na terra.",
      "palavras": [
        { "hebraico": "כִּי תַעֲבֹד אֶת הָאֲדָמָה", "transliteral": "KI TA'AVOD ET HA'ADAMAH", "traducao_palavra": "Quando lavrares a terra" },
        { "hebraico": "לֹא תֹסֵף תֵּת כֹּחָהּ לָךְ", "transliteral": "LO TOSEF TET KOCHA LACH", "traducao_palavra": "não te dará mais a sua força" },
        { "hebraico": "נָע וָנָד תִּהְיֶה בָּאָרֶץ", "transliteral": "NA VA'NAD TIHYEH BA'ARETZ", "traducao_palavra": "fugitivo e errante serás na terra" }
      ]
    },
    "13": {
      "traducao_completa": "E disse Caim ao SENHOR: É maior a minha punição do que a que posso suportar.",
      "palavras": [
        { "hebraico": "וַיֹּאמֶר קַיִן אֶל יְהוָה", "transliteral": "VAYOMER QAYIN EL YHVH", "traducao_palavra": "E disse Caim ao SENHOR" },
        { "hebraico": "גָּדוֹל עֲוֹנִי מִנְּשֹׁא", "transliteral": "GADOL AVONI MINNASO", "traducao_palavra": "É maior a minha punição" },
        { "hebraico": "מִסְּלֹחַ", "transliteral": "MISLOACH", "traducao_palavra": "do que a que posso suportar" }
      ]
    },
    "14": {
      "traducao_completa": "Eis que hoje me lanças fora da face da terra; e da tua face me esconderei, e serei fugitivo e errante na terra; e será que todo aquele que me achar me matará.",
      "palavras": [
        { "hebraico": "הֵן גֵּרַשְׁתָּ אֹתִי הַיּוֹם", "transliteral": "HEN GERASHTA OTI HAYYOM", "traducao_palavra": "Eis que hoje me lanças fora" },
        { "hebraico": "מֵעַל פְּנֵי הָאֲדָמָה", "transliteral": "ME'AL PNEI HA'ADAMAH", "traducao_palavra": "da face da terra" },
        { "hebraico": "וּמִפָּנֶיךָ אֶסָּתֵר", "transliteral": "UMIPANECHA ESSATER", "traducao_palavra": "e da tua face me esconderei" },
        { "hebraico": "וְהָיִיתִי נָע וָנָד בָּאָרֶץ", "transliteral": "VEHAYITI NA VA'NAD BA'ARETZ", "traducao_palavra": "e serei fugitivo e errante na terra" },
        { "hebraico": "וְהָיָה כָל מֹצְאִי יַהַרְגֵנִי", "transliteral": "VEHAYA KOL MOTZI YAHARGENI", "traducao_palavra": "e será que todo aquele que me achar me matará" }
      ]
    },
    "15": {
      "traducao_completa": "E disse-lhe o SENHOR: Portanto, qualquer que matar a Caim, sete vezes será castigado. E pôs o SENHOR um sinal em Caim, para que quem o encontrasse não o matasse.",
      "palavras": [
        { "hebraico": "וַיֹּאמֶר לוֹ יְהוָה", "transliteral": "VAYOMER LO YHVH", "traducao_palavra": "E disse-lhe o SENHOR" },
        { "hebraico": "לָכֵן כָּל הֹרֵג קַיִן", "transliteral": "LACHEN KOL HOREG QAYIN", "traducao_palavra": "Portanto, qualquer que matar a Caim" },
        { "hebraico": "שִׁבְעָתַיִם יֻקָּם", "transliteral": "SHIV'ATAYIM YUQQAM", "traducao_palavra": "sete vezes será castigado" },
        { "hebraico": "וַיָּשֶׂם יְהוָה לְקַיִן אוֹת", "transliteral": "VAYASEM YHVH LEQAYIN OT", "traducao_palavra": "E pôs o SENHOR um sinal em Caim" },
        { "hebraico": "לְבִלְתִּי הַכּוֹת אֹתוֹ", "transliteral": "LEVILTI HAKKOT OTO", "traducao_palavra": "para que quem o encontrasse não o matasse" }
      ]
    },
    "16": {
      "traducao_completa": "E saiu Caim de diante da face do SENHOR, e habitou na terra de Node, ao oriente do Éden.",
      "palavras": [
        { "hebraico": "וַיֵּצֵא קַיִן מִלִּפְנֵי יְהוָה", "transliteral": "VAYETZE QAYIN MILIFNEI YHVH", "traducao_palavra": "E saiu Caim de diante da face do SENHOR" },
        { "hebraico": "וַיֵּשֶׁב בְּאֶרֶץ נוֹד", "transliteral": "VAYYESHEV BE'ERETZ NOD", "traducao_palavra": "e habitou na terra de Node" },
        { "hebraico": "קִדְמַת עֵדֶן", "transliteral": "QIDMAT EDEN", "traducao_palavra": "ao oriente do Éden" }
      ]
    },
    "17": {
      "traducao_completa": "E conheceu Caim a sua mulher, e ela concebeu, e deu à luz a Enoque; e ele edificou uma cidade, e chamou o nome da cidade conforme o nome de seu filho, Enoque.",
      "palavras": [
        { "hebraico": "וַיֵּדַע קַיִן אֶת אִשְׁתּוֹ", "transliteral": "VAYEDA QAYIN ET ISHTO", "traducao_palavra": "E conheceu Caim a sua mulher" },
        { "hebraico": "וַתַּהַר", "transliteral": "VATTAHAR", "traducao_palavra": "e ela concebeu" },
        { "hebraico": "וַתֵּלֶד אֶת חֲנוֹךְ", "transliteral": "VATTELED ET CHANOKH", "traducao_palavra": "e deu à luz a Enoque" },
        { "hebraico": "וַיְהִי בֹּנֶה עִיר", "transliteral": "VAYHI BONER IR", "traducao_palavra": "e ele edificou uma cidade" },
        { "hebraico": "וַיִּקְרָא שֵׁם הָעִיר", "transliteral": "VAYYIQQRA SHEM HA'IR", "traducao_palavra": "e chamou o nome da cidade" },
        { "hebraico": "כְּשֵׁם בְּנוֹ חֲנוֹךְ", "transliteral": "KESHEM BENO CHANOKH", "traducao_palavra": "conforme o nome de seu filho, Enoque" }
      ]
    },
    "18": {
      "traducao_completa": "E nasceu a Enoque, Irade; e Irade gerou a Meujael; e Meujael gerou a Metusael; e Metusael gerou a Lameque.",
      "palavras": [
        { "hebraico": "וַיִּוָּלֵד לַחֲנוֹךְ אֶת עִירָד", "transliteral": "VAYYIVALED LACHANOKH ET IRAD", "traducao_palavra": "E nasceu a Enoque, Irade" },
        { "hebraico": "וְעִירָד הוֹלִיד אֶת מְחוּיָאֵל", "transliteral": "VE'IRAD HOLID ET MECHUYA'EL", "traducao_palavra": "e Irade gerou a Meujael" },
        { "hebraico": "וּמְחוּיָאֵל הוֹלִיד אֶת מְתוּשָׁאֵל", "transliteral": "UMECHUYA'EL HOLID ET METUSHA'EL", "traducao_palavra": "e Meujael gerou a Metusael" },
        { "hebraico": "וּמְתוּשָׁאֵל הוֹלִיד אֶת לֶמֶךְ", "transliteral": "UMETUSHA'EL HOLID ET LEMECH", "traducao_palavra": "e Metusael gerou a Lameque" }
      ]
    },
    "19": {
      "traducao_completa": "E tomou Lameque duas mulheres; o nome de uma era Ada, e o nome da outra Zilá.",
      "palavras": [
        { "hebraico": "וַיִּקַּח לוֹ לֶמֶךְ", "transliteral": "VAYIQQACH LO LEMECH", "traducao_palavra": "E tomou Lameque" },
        { "hebraico": "שְׁתֵּי נָשִׁים", "transliteral": "SHTEI NASHIM", "traducao_palavra": "duas mulheres" },
        { "hebraico": "שֵׁם הָאַחַת עָדָה", "transliteral": "SHEM HA'ACHAT ADAH", "traducao_palavra": "o nome de uma era Ada" },
        { "hebraico": "וְשֵׁם הַשֵּׁנִית צִלָּה", "transliteral": "VESHEM HASHENIT TZILLAH", "traducao_palavra": "e o nome da outra Zilá" }
      ]
    },
    "20": {
      "traducao_completa": "E Ada deu à luz a Jabal; este foi o pai dos que habitam em tendas e possuem gado.",
      "palavras": [
        { "hebraico": "וַתֵּלֶד עָדָה אֶת יָבָל", "transliteral": "VATTELED ADAH ET YAVAL", "traducao_palavra": "E Ada deu à luz a Jabal" },
        { "hebraico": "הוּא הָיָה אֲבִי", "transliteral": "HU HAYA AVI", "traducao_palavra": "este foi o pai" },
        { "hebraico": "יוֹשֵׁב אֹהֶל וּמִקְנֶה", "transliteral": "YOSHEV OHEL UMIQNEH", "traducao_palavra": "dos que habitam em tendas e possuem gado" }
