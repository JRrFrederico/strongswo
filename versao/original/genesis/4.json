    "21": {
      "traducao_completa": "E o nome de seu irmão era Jubal; este foi o pai de todos os que tocam harpa e órgão.",
      "palavras": [
        { "hebraico": "וְשֵׁם אָחִיו יוּבָל", "transliteral": "VESHEM ACHIYV YUBAL", "traducao_palavra": "E o nome de seu irmão era Jubal" },
        { "hebraico": "הוּא הָיָה אֲבִי כָּל", "transliteral": "HU HAYA AVI KOL", "traducao_palavra": "este foi o pai de todos" },
        { "hebraico": "תֹּפֵשׂ כִּנּוֹר וְעוּגָב", "transliteral": "TOFES KINNOR VE'UGAB", "traducao_palavra": "que tocam harpa e órgão" }
      ]
