    "22": {
      "traducao_completa": "E Zilá também deu à luz a Tubal-Caim, artífice de todo instrumento cortante de cobre e de ferro; e a irmã de Tubal-Caim foi Naamá.",
      "palavras": [
        { "hebraico": "וְצִלָּה גַם הִיא יָלְדָה", "transliteral": "VETZILLAH GAM HI YALDAH", "traducao_palavra": "E Zilá também deu à luz" },
        { "hebraico": "אֶת תּוּבַל קַיִן", "transliteral": "ET TUBAL QAYIN", "traducao_palavra": "a Tubal-Caim" },
        { "hebraico": "לֹטֵשׁ כָּל חֹרֵשׁ נְחֹשֶׁת וּבַרְזֶל", "transliteral": "LOTESH KOL CHORESH NECHOCHET UVARZEL", "traducao_palavra": "artífice de todo instrumento cortante de cobre e de ferro" },
        { "hebraico": "וַאֲחוֹת תּוּבַל קַיִן נַעֲמָה", "transliteral": "VE'ACHOT TUBAL QAYIN NA'AMAH", "traducao_palavra": "e a irmã de Tubal-Caim foi Naamá" }
      ]
    },
    "23": {
      "traducao_completa": "E disse Lameque a suas mulheres: Ada e Zilá, ouvi a minha voz; mulheres de Lameque, escutai o meu dito: porquanto matei um homem por me ferir, e um mancebo por me pisar;",
      "palavras": [
        { "hebraico": "וַיֹּאמֶר לֶמֶךְ לְנָשָׁיו", "transliteral": "VAYOMER LEMECH LENASHAV", "traducao_palavra": "E disse Lameque a suas mulheres" },
        { "hebraico": "עָדָה וְצִלָּה שְׁמַעַן קוֹלִי", "transliteral": "ADAH VETZILLAH SHAMA'AN QOLI", "traducao_palavra": "Ada e Zilá, ouvi a minha voz" },
        { "hebraico": "נְשֵׁי לֶמֶךְ הַאֲזִינָה אִמְרָתִי", "transliteral": "NISHEI LEMECH HA'AZINA IMRATI", "traducao_palavra": "mulheres de Lameque, escutai o meu dito" },
        { "hebraico": "כִּי אִישׁ הָרַגְתִּי לְפִצְעִי", "transliteral": "KI ISH HARAGTI LEFITZI", "traducao_palavra": "porquanto matei um homem por me ferir" },
        { "hebraico": "וְיֶלֶד לְחַבֻּרָתִי", "transliteral": "VEYELD LECHABURATI", "traducao_palavra": "e um mancebo por me pisar" }
      ]
    },
    "24": {
      "traducao_completa": "Se Caim será vingado sete vezes, verdadeiramente Lameque setenta vezes sete.",
      "palavras": [
        { "hebraico": "כִּי שִׁבְעָתַיִם יֻקָּם קָיִן", "transliteral": "KI SHIV'ATAYIM YUQQAM QAYIN", "traducao_palavra": "Se Caim será vingado sete vezes" },
        { "hebraico": "וְלֶמֶךְ שִׁבְעִים וְשִׁבְעָה", "transliteral": "VELEMECH SHIV'IM VESHIV'AH", "traducao_palavra": "verdadeiramente Lameque setenta vezes sete" }
      ]
    },
    "25": {
      "traducao_completa": "E tornou Adão a conhecer a sua mulher; e ela deu à luz um filho, e chamou o seu nome Sete; porque, disse ela, Deus me deu outro descendente em lugar de Abel, porquanto Caim o matou.",
      "palavras": [
        { "hebraico": "וַיֵּדַע אָדָם עוֹד", "transliteral": "VAYEDA ADAM OD", "traducao_palavra": "E tornou Adão a conhecer" },
        { "hebraico": "אֶת אִשְׁתּוֹ", "transliteral": "ET ISHTO", "traducao_palavra": "a sua mulher" },
        { "hebraico": "וַתֵּלֶד בֵּן", "transliteral": "VATTELED BEN", "traducao_palavra": "e ela deu à luz um filho" },
        { "hebraico": "וַתִּקְרָא אֶת שְׁמוֹ שֵׁת", "transliteral": "VATTIQRA ET SHMO SHETH", "traducao_palavra": "e chamou o seu nome Sete" },
        { "hebraico": "כִּי שָׁת לִי אֱלֹהִים", "transliteral": "KI SHAT LI ELOHIM", "traducao_palavra": "porque, disse ela, Deus me deu" },
        { "hebraico": "זֶרַע אַחֵר", "transliteral": "ZERA ACHER", "traducao_palavra": "outro descendente" },
        { "hebraico": "תַּחַת הֶבֶל", "transliteral": "TACHAT HEVEL", "traducao_palavra": "em lugar de Abel" },
        { "hebraico": "כִּי הֲרָגוֹ קָיִן", "transliteral": "KI HARAGO QAYIN", "traducao_palavra": "porquanto Caim o matou" }
      ]
    },
    "26": {
      "traducao_completa": "E a Sete também nasceu um filho; e chamou o seu nome Enos; então se começou a invocar o nome do SENHOR.",
      "palavras": [
        { "hebraico": "וּלְשֵׁת גַּם הוּא יֻלַּד בֵּן", "transliteral": "ULESHETH GAM HU YULAD BEN", "traducao_palavra": "E a Sete também nasceu um filho" },
        { "hebraico": "וַיִּקְרָא אֶת שְׁמוֹ אֱנוֹשׁ", "transliteral": "VAYYIQQRA ET SHMO ENOSH", "traducao_palavra": "e chamou o seu nome Enos" },
        { "hebraico": "אָז הוּחַל לִקְרֹא בְּשֵׁם יְהוָה", "transliteral": "AZ HUCHAL LIQRO BESHEM YHVH", "traducao_palavra": "então se começou a invocar o nome do SENHOR" }
