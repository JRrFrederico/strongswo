        { "hebraico": "וַחֲוִילָה", "transliteral": "vachavilah", "traducao_palavra": "e Havilá" },
        { "hebraico": "וְסַבְתָּה", "transliteral": "vesabtah", "traducao_palavra": "e Sabtá" },
