        { "hebraico": "תּוֹלְדֹת", "transliteral": "toledot", "traducao_palavra": "são as gerações" },
        { "hebraico": "בְּנֵי", "transliteral": "bney", "traducao_palavra": "dos filhos" },
        { "hebraico": "נֹחַ", "transliteral": "noach", "traducao_palavra": "de Noé" },
        { "hebraico": "שֵׁם", "transliteral": "shem", "traducao_palavra": "Sem" },
        { "hebraico": "חָם", "transliteral": "cham", "traducao_palavra": "Cam" },
        { "hebraico": "וְיָפֶת", "transliteral": "veyafet", "traducao_palavra": "e Jafé" },
        { "hebraico": "וַיִּוָּלְדוּ", "transliteral": "vayivalədu", "traducao_palavra": "e nasceram-lhes" },
        { "hebraico": "לָהֶם", "transliteral": "lahem", "traducao_palavra": "filhos" },
        { "hebraico": "בָנִים", "transliteral": "vanim", "traducao_palavra": "" },
        { "hebraico": "אַחַר", "transliteral": "'achar", "traducao_palavra": "depois" },
        { "hebraico": "הַמַּבּוּל", "transliteral": "hamabul", "traducao_palavra": "do dilúvio" }
      ]
    },
    "2": {
