        { "hebraico": "אִיֵּי", "transliteral": "'iye", "traducao_palavra": "as ilhas" },
        { "hebraico": "הַגּוֹיִם", "transliteral": "hagoyim", "traducao_palavra": "das nações" },
        { "hebraico": "בְּאַרְצֹתָם", "transliteral": "be'artsotam", "traducao_palavra": "nas suas terras" },
        { "hebraico": "אִישׁ", "transliteral": "'ish", "traducao_palavra": "cada um" },
        { "hebraico": "לְשֹׁנֹו", "transliteral": "leshono", "traducao_palavra": "segundo a sua língua" },
