      "palavras": [
        { "hebraico": "הוּא", "transliteral": "hu'", "traducao_palavra": "Ele" },
        { "hebraico": "הָיָה", "transliteral": "hayah", "traducao_palavra": "foi" },
        { "hebraico": "גִּבֹּר", "transliteral": "gibbor", "traducao_palavra": "poderoso" },
        { "hebraico": "צַיִד", "transliteral": "tsayid", "traducao_palavra": "caçador" },
        { "hebraico": "לִפְנֵי", "transliteral": "lifney", "traducao_palavra": "diante de" },
        { "hebraico": "יְהוָה", "transliteral": "'adonay", "traducao_palavra": "SENHOR" },
