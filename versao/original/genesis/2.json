{
  "livro": "Gênesis",
  "capitulo": 2,
  "versiculos": {
    "1": {
      "traducao_completa": "Assim foram acabados os céus e a terra, e todo o exército deles.",
      "palavras": [
        { "hebraico": "וַיְכֻלּוּ", "transliteral": "VAYECHULLU", "traducao_palavra": "Assim foram acabados" },
        { "hebraico": "הַשָּׁמַיִם", "transliteral": "HASHAMAYIM", "traducao_palavra": "os céus" },
        { "hebraico": "וְהָאָרֶץ", "transliteral": "VEHA'ARETZ", "traducao_palavra": "e a terra" },
        { "hebraico": "וְכָל צְבָאָם", "transliteral": "VECHOL TZAVA'AM", "traducao_palavra": "e todo o exército deles" }
      ]
    },
    "2": {
      "traducao_completa": "E no sétimo dia Deus acabou a obra que tinha feito; e no sétimo dia descansou de toda a obra que tinha feito.",
      "palavras": [
        { "hebraico": "וַיְכַל אֱלֹהִים", "transliteral": "VAYECHAL ELOHIM", "traducao_palavra": "E no sétimo dia Deus acabou" },
        { "hebraico": "בַּיּוֹם הַשְּׁבִיעִי", "transliteral": "BAYYOM HASHVI'I", "traducao_palavra": "" },
        { "hebraico": "מְלַאכְתּוֹ אֲשֶׁר עָשָׂה", "transliteral": "MELACHTO ASHER ASAH", "traducao_palavra": "a obra que tinha feito" },
        { "hebraico": "וַיִּשְׁבֹּת בַּיּוֹם הַשְּׁבִיעִי", "transliteral": "VAYISHBOT BAYYOM HASHVI'I", "traducao_palavra": "e no sétimo dia descansou" },
        { "hebraico": "מִכָּל מְלַאכְתּוֹ אֲשֶׁר עָשָׂה", "transliteral": "MIKOL MELACHTO ASHER ASAH", "traducao_palavra": "de toda a obra que tinha feito" }
      ]
    },
    "3": {
      "traducao_completa": "E abençoou Deus o sétimo dia, e o santificou; porque nele descansou de toda a sua obra que Deus criara e fizera.",
      "palavras": [
        { "hebraico": "וַיְבָרֶךְ אֱלֹהִים", "transliteral": "VAYVARECH ELOHIM", "traducao_palavra": "E abençoou Deus" },
        { "hebraico": "אֶת יוֹם הַשְּׁבִיעִי", "transliteral": "ET YOM HASHVI'I", "traducao_palavra": "o sétimo dia" },
        { "hebraico": "וַיְקַדֵּשׁ אֹתוֹ", "transliteral": "VAYQADDESH OTO", "traducao_palavra": "e o santificou" },
        { "hebraico": "כִּי בוֹ שָׁבַת", "transliteral": "KI BO SHAVAT", "traducao_palavra": "porque nele descansou" },
        { "hebraico": "מִכָּל מְלַאכְתּוֹ", "transliteral": "MIKOL MELACHTO", "traducao_palavra": "de toda a sua obra" },
        { "hebraico": "אֲשֶׁר בָּרָא אֱלֹהִים", "transliteral": "ASHER BARA ELOHIM", "traducao_palavra": "que Deus criara" },
        { "hebraico": "לַעֲשׂוֹת", "transliteral": "LA'ASOT", "traducao_palavra": "e fizera" }
      ]
    },
    "4": {
      "traducao_completa": "Estes são os feitos dos céus e da terra, quando foram criados, no dia em que o SENHOR Deus fez a terra e os céus,",
      "palavras": [
        { "hebraico": "אֵלֶּה תוֹלְדוֹת", "transliteral": "EILEH TOLDOT", "traducao_palavra": "Estes são os feitos" },
        { "hebraico": "הַשָּׁמַיִם וְהָאָרֶץ", "transliteral": "HASHAMAYIM VEHA'ARETZ", "traducao_palavra": "dos céus e da terra" },
        { "hebraico": "בְּהִבָּרְאָם", "transliteral": "BEHIBBAR'AM", "traducao_palavra": "quando foram criados" },
        { "hebraico": "בְּיוֹם עֲשׂוֹת", "transliteral": "BAYYOM ASOT", "traducao_palavra": "no dia em que" },
        { "hebraico": "יְהוָה אֱלֹהִים", "transliteral": "YHVH ELOHIM", "traducao_palavra": "o SENHOR Deus" },
        { "hebraico": "אֶרֶץ וְשָׁמָיִם", "transliteral": "ERETZ VESHAMAYIM", "traducao_palavra": "fez a terra e os céus" }
      ]
    },
    "5": {
      "traducao_completa": "E toda planta do campo ainda não estava na terra, e toda erva do campo ainda não brotava; porque o SENHOR Deus não tinha feito chover sobre a terra, e não havia homem para lavrar a terra.",
      "palavras": [
        { "hebraico": "וְכֹל שִׂיחַ הַשָּׂדֶה", "transliteral": "VECHOL SIACH HASSEDEH", "traducao_palavra": "E toda planta do campo" },
        { "hebraico": "טֶרֶם יִהְיֶה בָּאָרֶץ", "transliteral": "TEREM YIHYEH BA'ARETZ", "traducao_palavra": "ainda não estava na terra" },
        { "hebraico": "וְכֹל עֵשֶׂב הַשָּׂדֶה", "transliteral": "VECHOL ESEV HASSEDEH", "traducao_palavra": "e toda erva do campo" },
        { "hebraico": "טֶרֶם יִצְמָח", "transliteral": "TEREM YITZMACHA", "traducao_palavra": "ainda não brotava" },
        { "hebraico": "כִּי לֹא הִמְטִיר", "transliteral": "KI LO HIMTIR", "traducao_palavra": "porque o SENHOR Deus não tinha feito chover" },
        { "hebraico": "יְהוָה אֱלֹהִים", "transliteral": "YHVH ELOHIM", "traducao_palavra": "" },
        { "hebraico": "עַל הָאָרֶץ", "transliteral": "AL HA'ARETZ", "traducao_palavra": "sobre a terra" },
        { "hebraico": "וְאָדָם אַיִן", "transliteral": "VE'ADAM AYIN", "traducao_palavra": "e não havia homem" },
        { "hebraico": "לַעֲבֹד אֶת הָאֲדָמָה", "transliteral": "LA'AVOD ET HA'ADAMAH", "traducao_palavra": "para lavrar a terra" }
      ]
    },
    "6": {
      "traducao_completa": "Mas subia um vapor da terra, e regava toda a face da terra.",
      "palavras": [
        { "hebraico": "וְאֵד", "transliteral": "VE'ED", "traducao_palavra": "Mas subia um vapor" },
        { "hebraico": "יַעֲלֶה מִן הָאָרֶץ", "transliteral": "YA'ALEH MIN HA'ARETZ", "traducao_palavra": "da terra" },
        { "hebraico": "וְהִשְׁקָה", "transliteral": "VEHISHQA", "traducao_palavra": "e regava" },
        { "hebraico": "אֶת כָּל פְּנֵי הָאֲדָמָה", "transliteral": "ET KOL PENEI HA'ADAMAH", "traducao_palavra": "toda a face da terra" }
      ]
    },
    "7": {
      "traducao_completa": "E formou o SENHOR Deus o homem do pó da terra, e soprou em seus narizes o fôlego da vida; e o homem tornou-se alma vivente.",
      "palavras": [
        { "hebraico": "וַיִּיצֶר יְהוָה אֱלֹהִים", "transliteral": "VAYITZER YHVH ELOHIM", "traducao_palavra": "E formou o SENHOR Deus" },
        { "hebraico": "אֶת הָאָדָם", "transliteral": "ET HA'ADAM", "traducao_palavra": "o homem" },
        { "hebraico": "עָפָר מִן הָאֲדָמָה", "transliteral": "AFAR MIN HA'ADAMAH", "traducao_palavra": "do pó da terra" },
        { "hebraico": "וַיִּפַּח בְּאַפָּיו", "transliteral": "VAYIPPAH BE'APAV", "traducao_palavra": "e soprou em seus narizes" },
        { "hebraico": "נִשְׁמַת חַיִּים", "transliteral": "NISHMAT CHAYYIM", "traducao_palavra": "o fôlego da vida" },
        { "hebraico": "וַיְהִי הָאָדָם", "transliteral": "VAYHI HA'ADAM", "traducao_palavra": "e o homem tornou-se" },
        { "hebraico": "לְנֶפֶשׁ חַיָּה", "transliteral": "LENEFESH CHAYYAH", "traducao_palavra": "alma vivente" }
      ]
    },
    "8": {
      "traducao_completa": "E plantou o SENHOR Deus um jardim no Éden, da banda do oriente; e pôs ali o homem que tinha formado.",
      "palavras": [
        { "hebraico": "וַיִּטַּע יְהוָה אֱלֹהִים", "transliteral": "VAYITTA YHVH ELOHIM", "traducao_palavra": "E plantou o SENHOR Deus" },
        { "hebraico": "גַּן בְּעֵדֶן", "transliteral": "GAN BE'EDEN", "traducao_palavra": "um jardim no Éden" },
        { "hebraico": "מִקֶּדֶם", "transliteral": "MIQQEDDEM", "traducao_palavra": "da banda do oriente" },
        { "hebraico": "וַיָּשֶׂם שָׁם", "transliteral": "VAYASEM SHAM", "traducao_palavra": "e pôs ali" },
        { "hebraico": "אֶת הָאָדָם", "transliteral": "ET HA'ADAM", "traducao_palavra": "o homem" },
        { "hebraico": "אֲשֶׁר יָצָר", "transliteral": "ASHER YATZAR", "traducao_palavra": "que tinha formado" }
      ]
    },
    "9": {
      "traducao_completa": "E o SENHOR Deus fez brotar da terra toda árvore agradável à vista, e boa para comida; e a árvore da vida no meio do jardim, e a árvore do conhecimento do bem e do mal.",
      "palavras": [
        { "hebraico": "וַיַּצְמַח יְהוָה אֱלֹהִים", "transliteral": "VAYATZMACH YHVH ELOHIM", "traducao_palavra": "E o SENHOR Deus fez brotar" },
        { "hebraico": "מִן הָאֲדָמָה", "transliteral": "MIN HA'ADAMAH", "traducao_palavra": "da terra" },
        { "hebraico": "כָּל עֵץ נֶחְמָד", "transliteral": "KOL ETZ NECHMAD", "traducao_palavra": "toda árvore agradável" },
        { "hebraico": "לְמַרְאֶה", "transliteral": "LEMAR'EH", "traducao_palavra": "à vista" },
        { "hebraico": "וְטוֹב לְמַאֲכָל", "transliteral": "VETOV LE'ACHAL", "traducao_palavra": "e boa para comida" },
        { "hebraico": "וְעֵץ הַחַיִּים", "transliteral": "VE'ETZ HACHAYYIM", "traducao_palavra": "e a árvore da vida" },
        { "hebraico": "בְּתוֹךְ הַגָּן", "transliteral": "BETOCH HAGAN", "traducao_palavra": "no meio do jardim" },
        { "hebraico": "וְעֵץ דַּעַת", "transliteral": "VE'ETZ DA'AT", "traducao_palavra": "e a árvore do conhecimento" },
        { "hebraico": "טוֹב וָרָע", "transliteral": "TOV VARA", "traducao_palavra": "do bem e do mal" }
      ]
    },
    "10": {
      "traducao_completa": "E saía um rio do Éden para regar o jardim; e dali se dividia e se tornava em quatro braços.",
      "palavras": [
        { "hebraico": "וְנָהָר יֹצֵא מֵעֵדֶן", "transliteral": "VENAHAR YOTZE ME'EDEN", "traducao_palavra": "E saía um rio do Éden" },
        { "hebraico": "לְהַשְׁקוֹת אֶת הַגָּן", "transliteral": "LEHASHQOT ET HAGAN", "traducao_palavra": "para regar o jardim" },
        { "hebraico": "וּמִשָּׁם יִפָּרֵד", "transliteral": "UMISHAM YIPPARED", "traducao_palavra": "e dali se dividia" },
        { "hebraico": "וְהָיָה לְאַרְבָּעָה רָאשִׁים", "transliteral": "VEHAYAH LE'ARBA'AH ROSHIM", "traducao_palavra": "e se tornava em quatro braços" }
      ]
    },
    "11": {
      "traducao_completa": "O nome do primeiro é Pisom; este é o que rodeia toda a terra de Havilá, onde há ouro;",
      "palavras": [
        { "hebraico": "שֵׁם הָאֶחָד פִּישׁוֹן", "transliteral": "SHEM HA'ECHAD PISHON", "traducao_palavra": "O nome do primeiro é Pisom" },
        { "hebraico": "הוּא הַסֹּבֵב", "transliteral": "HU HASOVEV", "traducao_palavra": "este é o que rodeia" },
        { "hebraico": "אֵת כָּל אֶרֶץ הַחֲוִילָה", "transliteral": "ET KOL ERETZ CHAVILAH", "traducao_palavra": "toda a terra de Havilá" },
        { "hebraico": "אֲשֶׁר שָׁם הַזָּהָב", "transliteral": "ASHER SHAM HAZAHAV", "traducao_palavra": "onde há ouro" }
      ]
    },
    "12": {
      "traducao_completa": "E o ouro dessa terra é bom; ali há o bdélio e a pedra de ônix.",
      "palavras": [
        { "hebraico": "וּזְהַב הָאָרֶץ הַהִוא טוֹב", "transliteral": "UZEHAV HA'ARETZ HAHI TOV", "traducao_palavra": "E o ouro dessa terra é bom" },
        { "hebraico": "שָׁם הַבְּדֹלַח", "transliteral": "SHAM HABEDOLACH", "traducao_palavra": "ali há o bdélio" },
        { "hebraico": "וְאֶבֶן הַשֹּׁהַם", "transliteral": "VE'EVEN HASHOHAM", "traducao_palavra": "e a pedra de ônix" }
      ]
    },
    "13": {
      "traducao_completa": "E o nome do segundo rio é Giom; este é o que rodeia toda a terra de Cuxe.",
      "palavras": [
        { "hebraico": "וְשֵׁם הַנָּהָר הַשֵּׁנִי גִּיחוֹ", "transliteral": "VESHEM HANAHAR HASHENI GIYCHON", "traducao_palavra": "E o nome do segundo rio é Giom" },
        { "hebraico": "הוּא הַסּוֹבֵב", "transliteral": "HU HASOVEV", "traducao_palavra": "este é o que rodeia" },
        { "hebraico": "אֵת כָּל אֶרֶץ כּוּשׁ", "transliteral": "ET KOL ERETZ KUSH", "traducao_palavra": "toda a terra de Cuxe" }
      ]
    },
    "14": {
      "traducao_completa": "E o nome do terceiro rio é Hidequel; este é o que vai para a banda do oriente da Assíria. E o quarto rio é o Eufrates.",
      "palavras": [
        { "hebraico": "וְשֵׁם הַנָּהָר הַשְּׁלִישִׁי חִדֶּקֶל", "transliteral": "VESHEM HANAHAR HASHLISHI CHIDDEQEL", "traducao_palavra": "E o nome do terceiro rio é Hidequel" },
        { "hebraico": "הוּא הַהוֹלֵךְ", "transliteral": "HU HAHOLECH", "traducao_palavra": "este é o que vai" },
        { "hebraico": "קִדְמַת אַשּׁוּר", "transliteral": "QIDMAT ASHUR", "traducao_palavra": "para a banda do oriente da Assíria" },
        { "hebraico": "וְהַנָּהָר הָרְבִיעִי הוּא פְרָת", "transliteral": "VEHANAHAR HARVI'I HU PERAT", "traducao_palavra": "E o quarto rio é o Eufrates" }
      ]
    },
    "15": {
      "traducao_completa": "E tomou o SENHOR Deus o homem, e o pôs no jardim do Éden para o cultivar e o guardar.",
      "palavras": [
        { "hebraico": "וַיִּקַּח יְהוָה אֱלֹהִים", "transliteral": "VAYIQQACH YHVH ELOHIM", "traducao_palavra": "E tomou o SENHOR Deus" },
        { "hebraico": "אֶת הָאָדָם", "transliteral": "ET HA'ADAM", "traducao_palavra": "o homem" },
        { "hebraico": "וַיַּנִּחֵהוּ", "transliteral": "VAYANICHEHU", "traducao_palavra": "e o pôs" },
        { "hebraico": "בְּגַן עֵדֶן", "transliteral": "BEGAN EDEN", "traducao_palavra": "no jardim do Éden" },
        { "hebraico": "לְעָבְדָהּ וּלְשָׁמְרָהּ", "transliteral": "LE'AVDAH ULESHAMRAH", "traducao_palavra": "para o cultivar e o guardar" }
      ]
    },
    "16": {
      "traducao_completa": "E ordenou o SENHOR Deus ao homem, dizendo: De toda árvore do jardim comerás livremente;",
      "palavras": [
        { "hebraico": "וַיְצַו יְהוָה אֱלֹהִים", "transliteral": "VAYTZAV YHVH ELOHIM", "traducao_palavra": "E ordenou o SENHOR Deus" },
        { "hebraico": "עַל הָאָדָם", "transliteral": "AL HA'ADAM", "traducao_palavra": "ao homem" },
        { "hebraico": "לֵאמֹר", "transliteral": "LEMOR", "traducao_palavra": "dizendo" },
        { "hebraico": "מִכֹּל עֵץ הַגָּן", "transliteral": "MIKOL ETZ HAGAN", "traducao_palavra": "De toda árvore do jardim" },
        { "hebraico": "אָכֹל תֹּאכֵל", "transliteral": "ACHOL TOCHEL", "traducao_palavra": "comerás livremente" }
      ]
    },
    "17": {
      "traducao_completa": "Mas da árvore do conhecimento do bem e do mal não comerás; porque no dia em que dela comeres, certamente morrerás.",
      "palavras": [
        { "hebraico": "וּמֵעֵץ הַדַּעַת טוֹב וָרָע", "transliteral": "UME'ETZ HADA'AT TOV VARA", "traducao_palavra": "Mas da árvore do conhecimento do bem e do mal" },
        { "hebraico": "לֹא תֹאכַל מִמֶּנּוּ", "transliteral": "LO TOCHAL MIMENNU", "traducao_palavra": "não comerás" },
        { "hebraico": "כִּי בְּיוֹם אֲכָלְךָ מִמֶּנּוּ", "transliteral": "KI BEYOM ACHALCHA MIMENNU", "traducao_palavra": "porque no dia em que dela comeres" },
        { "hebraico": "מוֹת תָּמוּת", "transliteral": "MOT TAMUT", "traducao_palavra": "certamente morrerás" }
      ]
    },
    "18": {
      "traducao_completa": "E disse o SENHOR Deus: Não é bom que o homem esteja só; far-lhe-ei uma adjutora que esteja como seu par.",
      "palavras": [
        { "hebraico": "וַיֹּאמֶר יְהוָה אֱלֹהִים", "transliteral": "VAYOMER YHVH ELOHIM", "traducao_palavra": "E disse o SENHOR Deus" },
        { "hebraico": "לֹא טוֹב הֱיוֹת הָאָדָם לְבַדּוֹ", "transliteral": "LO TOV HEYOT HA'ADAM LEVADO", "traducao_palavra": "Não é bom que o homem esteja só" },
        { "hebraico": "אֶעֱשֶׂה לּוֹ עֵזֶר", "transliteral": "E'ESEH LO EZER", "traducao_palavra": "far-lhe-ei uma adjutora" },
        { "hebraico": "כְּנֶגְדּוֹ", "transliteral": "KENEGDO", "traducao_palavra": "que esteja como seu par" }
      ]
    },
    "19": {
      "traducao_completa": "E da terra formou o SENHOR Deus todo animal do campo, e toda ave dos céus, e os trouxe ao homem para ver como este lhes chamaria; e tudo o que o homem chamou a cada alma vivente, esse é o seu nome.",
      "palavras": [
        { "hebraico": "וַיִּצֶר יְהוָה אֱלֹהִים", "transliteral": "VAYITZER YHVH ELOHIM", "traducao_palavra": "E da terra formou o SENHOR Deus" },
        { "hebraico": "מִן הָאֲדָמָה", "transliteral": "MIN HA'ADAMAH", "traducao_palavra": "" },
        { "hebraico": "כָּל חַיַּת הַשָּׂדֶה", "transliteral": "KOL CHAYYAT HASSEDEH", "traducao_palavra": "todo animal do campo" },
